{
    "platform": {
        "apiRuntime": "node:16"
    },
    "globalHeaders": {
        "content-security-policy": "default-src dancetime.io *.dancetime.io youtube.com *.youtube.com; upgrade-insecure-requests;",
        "X-Frame-Options": "SAMEORIGIN",
        "X-Permitted-Cross-Domain-Policies": "none",
        "Referrer-Policy":"no-referrer",
        "X-Content-Type-Options": "nosniff",
        "Permissions-Policy": "autoplay=()"
    }
}