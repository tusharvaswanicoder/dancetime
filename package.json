        "@tensorflow/tfjs-converter": "^3.11.0",
        "@tensorflow/tfjs-core": "^3.11.0",
        "autoprefixer": "^10.4.0",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.4",
        "prettier": "2.5.0",
        "rollup": "^2.60.1",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-postcss": "^4.0.2",
